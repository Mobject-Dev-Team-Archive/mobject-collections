VAR
	value : INT := 123;
	linkedList : LinkedList;
	handler : CollectionChangedEventHandler(ExpectedOriginator := linkedList);
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST('CheckOnceEvent');

// @TEST-FIXTURE
linkedList.OnceEvent('OnChanged',handler);
linkedList.AddFirst(value);
handler.Reset();

// @TEST-RUN
linkedList.AddFirst(value);

// @TEST-ASSSERT
